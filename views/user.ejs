<%- include header%>

<div>

<% posts.forEach(function(post,index) { %>
    <p>
        <h1>
            <a href="/u/<%= post.username%>/<%= post.time.day%>/<%= post.title%>">
                <%= post.title%>
            </a>
        </h1>
    </p>

    <p>
        Author: <a href="/u/<%= post.username%>"><%= post.username%></a>
        Date: <%= post.time.minute%>
    </p>

    <p>
        <%- post.post%>
    </p>
<% }) %>

</div>